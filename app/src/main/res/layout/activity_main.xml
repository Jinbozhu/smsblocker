<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_relativeLsyout_parent_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/activity_main_frameLayout_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/not_default_app"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Not default app"
            android:layout_gravity="center_horizontal|top" />

        <Button
            android:id="@+id/change_default_app"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Set to default app"
            android:layout_gravity="center" />
    </FrameLayout>



</RelativeLayout>
